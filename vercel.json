{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "api/**/*.js": {
          "runtime": "node@18",
          "cors": {
            "origin": "*"
          }
        }
    }
}