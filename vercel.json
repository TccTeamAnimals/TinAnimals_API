{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/users/login",
            "dest": "/server.js",
            "methods": ["POST"],
            "cors": {
              "origin": "*"
            }
        }
        ,{
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}